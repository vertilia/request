{
  "name": "vertilia/request",
  "description": "Simple library to abstract requests (CLI, HTTP, etc.)",
  "license": "BSD",
  "type": "library",
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "Vertilia\\Request\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": "^7.1",
    "vertilia/valid-array": "^1.0"
  },
  "suggest": {
    "phpunit/phpunit": "To run tests"
  }
}
